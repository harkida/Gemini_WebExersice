{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/roleplay_test.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/roleplay_admin.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/roleplay-admin",
      "dest": "api/roleplay_admin.py"
    },
    {
      "src": "/roleplay-session",
      "dest": "api/roleplay_admin.py"
    },
    {
      "src": "/roleplay-lobby",
      "dest": "api/roleplay_admin.py"
    },
    {
      "src": "/api/rp-admin/(.*)",
      "dest": "api/roleplay_admin.py"
    },
    {
  "src": "/api/rp-student/(.*)",
  "dest": "api/roleplay_admin.py"
    },
    {
      "src": "/roleplay-test",
      "dest": "api/roleplay_test.py"
    },
    {
      "src": "/api/analyst-test",
      "dest": "api/roleplay_test.py"
    },
    {
      "src": "/api/scenario-info",
      "dest": "api/roleplay_test.py"
    },
    {
      "src": "/api/analyst-test-audio",
      "dest": "api/roleplay_test.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}